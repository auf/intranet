<root>
    <dynamic-element name="diplomeName" type="text" index-type="not-searchable" repeatable="true">
        <dynamic-element name="diplomeType" type="list" index-type="not-searchable" repeatable="false">
            <dynamic-element name="Diplôme d'études collégiales (DEC)" type="DEC" repeatable="false"/>
            <dynamic-element name="Bacalauréat" type="BAC" repeatable="false"/>
            <dynamic-element name="Maîtrise" type="Maitrise" repeatable="false"/>
            <dynamic-element name="Doctorat" type="DOC" repeatable="false"/>
            <dynamic-element name="Autre Diplôme" type="Autre" repeatable="false"/>
            <meta-data>
            <entry name="displayAsTooltip">
                <![CDATA[true]]>
                </entry>
                <entry name="required">
                    <![CDATA[true]]>
                    </entry>
                    <entry name="instructions">
                        <![CDATA[]]>
                    </entry>
                    <entry name="label">
                        <![CDATA[Type de diplôme]]>
                    </entry>
                    <entry name="predefinedValue">
                        <![CDATA[]]>
                        </entry>
                    </meta-data>
                </dynamic-element>
                <dynamic-element name="diplomeDate" type="text" index-type="not-searchable" repeatable="false">
                    <meta-data>
                    <entry name="displayAsTooltip">
                        <![CDATA[false]]>
                    </entry>
                    <entry name="required">
                        <![CDATA[true]]>
                    </entry>
                    <entry name="instructions">
                        <![CDATA[Exemple de format accepter:
                            <br/>
                            2005
                            <br/>
                            2003 à 2006
                            <br/>
                            Depuis 2008]]>
                    </entry>
                <entry name="label"><![CDATA[Années de formation]]></entry>
                <entry name="predefinedValue"><![CDATA[19xx à 19xx]]></entry>
            </meta-data>
        </dynamic-element>
        <meta-data>
            <entry name="displayAsTooltip"><![CDATA[true]]></entry>
            <entry name="required"><![CDATA[true]]></entry>
            <entry name="instructions"><![CDATA[]]></entry>
            <entry name="label"><![CDATA[Nom du diplôme]]></entry>
            <entry name="predefinedValue"><![CDATA[]]></entry>
        </meta-data>
    </dynamic-element>
</root>